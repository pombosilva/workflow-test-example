#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/wrangler@4.24.1/node_modules/wrangler/bin/node_modules:/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/wrangler@4.24.1/node_modules/wrangler/node_modules:/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/wrangler@4.24.1/node_modules:/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/wrangler@4.24.1/node_modules/wrangler/bin/node_modules:/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/wrangler@4.24.1/node_modules/wrangler/node_modules:/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/wrangler@4.24.1/node_modules:/Users/osilva/Documents/test/small-snowflake-f12f/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wrangler.js" "$@"
else
  exec node  "$basedir/../../bin/wrangler.js" "$@"
fi
